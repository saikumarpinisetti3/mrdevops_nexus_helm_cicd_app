pipeline {
    agent any
    stages {
        stage('checkout') {
            steps {
                echo "checked out"
            }
        }
        stage('sonar quality check') {
            agent {
                docker {
                    image 'maven'
                    args '-u root'
                }
            }
            steps {
                script {
                    withSonarQubeEnv(credentialsId: 'token') {
                        // some block
                        sh 'mvn clean package sonar:sonar'
                    }
                }
            }
        }
                stage('sonar quality gate status'){
                steps{
                       script{
                        waitForQualityGate abortPipeline: false, credentialsId: 'token'
                       }
                }
            }
    }
}
